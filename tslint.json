{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "ban": [
      true,
      [
        "_",
        "extend"
      ],
      [
        "_",
        "isNull"
      ],
      [
        "_",
        "isDefined"
      ]
    ],
    "member-access": false,
    "trailing-comma": [
      false
    ],
    "object-literal-sort-keys": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "class-name": true,
    "comment-format": [
      true,
      "check-space",
      "check-lowercase"
    ],
    "curly": true,
    "eofline": true,
    "forin": true,
    "indent": [
      true,
      2
    ],
    "interface-name": [
      true
    ],
    "jsdoc-format": true,
    "label-position": true,
    "max-line-length": [
      false,
      150
    ],
    "member-ordering": [
      true,
      "public-before-private",
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-parameter-properties": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": false,
    "no-trailing-whitespace": false,
    "no-unused-variable": [
      true,
      "react"
    ],
    "no-use-before-declare": true,
    "no-var-requires": true,
    "no-console": [
      false
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-single"
    ],
    "radix": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": [
      false
    ],
    "typedef": [
      true
      // "callSignature",
      // "indexSignature",
      // "propertySignature",
      // "variableDeclarator"
    ],
    "array-type": [
      true,
      "array-simple"
    ],
    "jsx-no-multiline-js": false,
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any",
        "named-imports-order": "any"
      }
    ],
    "variable-name": [
      false
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}
